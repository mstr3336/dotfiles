# vim:ft=conf
Host jhpc
  ControlPath ~/.ssh/sockets/jhpc-%r@%h-%p

Host vhpc
  ControlPath ~/.ssh/sockets/vhpc-%r@%h-%p


Host vhpc jump jhpc
  ControlMaster auto
  ServerAliveInterval 30
  ServerAliveCountMax 3
  ForwardX11 yes
  # ForwardAgent yes



Host github.sydney.edu.au
  ControlMaster auto

